\begin_layout Section
Chebyshev (10 points)
\end_layout

\begin_layout Standard

\series bold
Compute the solution to the previous model when you use 8 Chebyshev polynomials
 on capital.
 Compare the solution with the one from previous questions.
\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $u(c,l)$
\end_inset

 denote the flow utility specified above, and 
\begin_inset Formula $u_{c}$
\end_inset

 denote the partial derivatives of the flow utility with respect to 
\begin_inset Formula $c$
\end_inset

 and 
\begin_inset Formula $l$
\end_inset

.
 From the envelope condition and FOC with respect to 
\begin_inset Formula $c$
\end_inset

 we get
\begin_inset Formula 
\begin{align*}
\mathbb{V}_{k}(z,\alpha,k) & =\frac{u_{c}(c,l)(\alpha e^{z}k^{\alpha-1}l^{1-\alpha}+1-\delta)}{(1-\psi)\mathbb{V}(z,\alpha,k)^{-\psi}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Plugging the above equation into the FOC with respect to 
\begin_inset Formula $k'$
\end_inset

 we get the Euler equation:
\begin_inset Formula 
\begin{align}
u_{c}(c,l) & =\beta(1-\psi)\Big[\mathbb{E}\Big(\mathbb{V}(z',\alpha',k')^{1-\gamma}\Big)\Big]^{\frac{\gamma-\psi}{1-\gamma}}\mathbb{E}\Bigg(\mathbb{V}(z',\alpha',k')^{-\gamma}\frac{u_{c}(c',l')(\alpha'e^{z'}k'^{\alpha'-1}l'^{1-\alpha'}+1-\delta)}{(1-\psi)\mathbb{V}(z',\alpha',k')^{-\psi}}\Bigg)\nonumber \\
\Rightarrow u_{c}(c,l) & =\beta\Big[\mathbb{E}\Big(\mathbb{V}(z',\alpha',k')^{1-\gamma}\Big)\Big]^{\frac{\gamma-\psi}{1-\gamma}}\mathbb{E}\Big(\mathbb{V}(z',\alpha',k')^{\psi-\gamma}u_{c}(c',l')(\alpha'e^{z'}k'^{\alpha'-1}l'^{1-\alpha'}+1-\delta)\Big)\label{eq:Euler}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
where 
\begin_inset Formula 
\begin{align*}
u(c,l) & =\Bigg(\log c-\eta\frac{l^{2}}{2}\Bigg)^{1-\psi}\\
u_{c}(c,l) & =(1-\psi)\Bigg(\log c-\eta\frac{l^{2}}{2}\Bigg)^{-\psi}\frac{1}{c}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $n$
\end_inset

 be the number of collocation points for Chebyshev polynomials.
 We approximate the decision rule for labor and value function as
\begin_inset Formula 
\begin{align}
l(z,\alpha,k|\theta_{i}^{l}) & =\sum_{i=1}^{n}\theta_{i}^{l}\psi_{i}^{l}(z,\alpha,k)\label{eq:labor approximate}\\
V(z,\alpha,k|\theta_{i}^{V}) & =\sum_{i=1}^{n}\theta_{i}^{V}\psi_{i}^{V}(z,\alpha,k)\label{eq:value approximate}
\end{align}

\end_inset

where 
\begin_inset Formula $\{\psi_{i}^{l}(z,\alpha,k)\}_{i=1}^{n},\,\{\psi_{i}^{V}(z,\alpha,k)\}_{i=1}^{n}$
\end_inset

 are basis functions for labor and value functions, and 
\begin_inset Formula $\{\theta_{i}^{l}\}_{i=1}^{n},\,\{\theta_{i}^{V}\}_{i=1}^{n}$
\end_inset

 are unknown coefficients.
\end_layout

\begin_layout Standard
Given the approximated policy function for 
\begin_inset Formula $l$
\end_inset

, we can use the equation 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:l and c"
plural "false"
caps "false"
noprefix "false"

\end_inset

 to derive optimal consumption:
\begin_inset Formula 
\begin{equation}
c(z,\alpha,k)=\frac{1-\alpha}{\eta}e^{z}k^{\alpha}l(z,\alpha,k|\theta_{i}^{l})^{-\alpha-1}\label{eq:consumption approximate}
\end{equation}

\end_inset

and from budget constraint, we get optimal capital for the next period:
\begin_inset Formula 
\begin{equation}
k'(z,\alpha,k)=e^{z}k_{t}^{\alpha}l(z,\alpha,k|\theta_{i}^{l})^{1-\alpha}-c(z,\alpha,k)+(1-\delta)k\label{eq:kp approximate}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
To solve for the coefficients, we plug equations 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:labor approximate"
plural "false"
caps "false"
noprefix "false"

\end_inset

, 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:value approximate"
plural "false"
caps "false"
noprefix "false"

\end_inset

, 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:consumption approximate"
plural "false"
caps "false"
noprefix "false"

\end_inset

 and 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:kp approximate"
plural "false"
caps "false"
noprefix "false"

\end_inset

 into the Euler equation 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:Euler"
plural "false"
caps "false"
noprefix "false"

\end_inset

, then we have
\begin_inset Formula 
\begin{align*}
(1-\psi)\Bigg(\log c-\eta\frac{l^{2}}{2}\Bigg)^{-\psi}\frac{1}{c} & =\beta\Big[\sum_{\alpha',z'}\Gamma_{\alpha_{'}|\alpha}\Pi_{z_{'}|z}\Big(\sum_{i=1}^{n}\theta_{i}^{V}\psi_{i}^{V}(z',\alpha',k')\Big)^{1-\gamma}\Big]^{\frac{1-\psi}{1-\gamma}}\\
 & \Big[\sum_{\alpha',z'}\Gamma_{\alpha_{'}|\alpha}\Pi_{z_{'}|z}\Big(\sum_{i=1}^{n}\theta_{i}^{V}\psi_{i}^{V}(z',\alpha',k')\Big)^{\psi-\gamma}(1-\psi)\Bigg(\log c'-\eta\frac{l'^{2}}{2}\Bigg)^{-\psi}\frac{1}{c'}(\alpha'e^{z'}k'^{\alpha'-1}l'^{1-\alpha'}+1-\delta)\Big]
\end{align*}

\end_inset

where 
\begin_inset Formula $c=c(z,\alpha,k),\,l=l(z,\alpha,k|\theta_{i}^{l}),\,c(z',\alpha')=c(z',\alpha',k'),\,l(z',\alpha')=l(z',\alpha',k')$
\end_inset

 and 
\begin_inset Formula $k'=k'(z,\alpha,k)$
\end_inset

.
\end_layout

\begin_layout Standard
Similarly, plugging the equations 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:labor approximate"
plural "false"
caps "false"
noprefix "false"

\end_inset

, 
\begin_inset CommandInset ref
LatexCommand ref
